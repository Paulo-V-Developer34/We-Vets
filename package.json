		"@biomejs/biome": "2.2.5",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"prisma": "^6.17.0",
		"tailwindcss": "^4",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5"
